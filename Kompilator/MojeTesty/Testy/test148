VAR
	a[4] b
BEGIN
	a[0] := 5;
	a[1] := 10;
	a[2] := 15;
	b := 7;
	a[3] := a[1] + b;
	IF 16 <> 5  THEN
		a[0] := a[0] - 1;
		WRITE 1;
	ELSE
		a[0] := 3 - a[0];
		WRITE 2;
		b := 0;
	ENDIF
	IF 98 <> 98 THEN
		WRITE 43;
	ELSE
		a[1] := a[0] - 2;
		WRITE 4;
	ENDIF
	IF 123 <> 124  THEN
		WRITE 12;
	ELSE
		WRITE 44;
	ENDIF
END

OUTPUT
1
4
12
ENDOUTPUT
